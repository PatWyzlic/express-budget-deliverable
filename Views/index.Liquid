        <body>
            <div class="container">
                <h1>Budgtr</h1>
                <table>
                    {%for budget in allBudgets%}
                        <tr>
                            <td>{{budget.date}} </td>
                            <td><a href="/budget/{{forloop.index0}}">{{budget.name}}</a></td>
                            <td>{{budget.amount}}</td>
                        </tr>
                    {%endfor%}
                </table>
                <a href="budget/new"><button>Create New Budget</button></a>
            </div>
        </body>
    </html>
